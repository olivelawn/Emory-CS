	(defun you-me-swap (word)
		(cond	((eq word 'I) 'you)
     		((eq word 'my) 'your)
     		((eq word 'me) 'you)
     		((eq word 'mine) 'yours)
     		((eq word 'am) 'are)
     		((eq word 'you) 'I)
     		((eq word 'your) 'my)
     		((eq word 'yours) 'mine)
     		((eq word 'are) 'am)
     		(t word)))

	(defun you-me	(phrase)
		(mapcar #'you-me-swap phrase))
